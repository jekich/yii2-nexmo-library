{
    "name": "jekich/yii2-nexmo-library",
    "description": "The Nexmo library for the Yii framework",
    "keywords": [
        "yii",
        "yii2",
        "yii 2",
        "nexmo"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/jekich/yii2-nexmo-library",
    "support": {
        "issues": "https://github.com/jekich/yii2-nexmo-library/issues",
        "source": "https://github.com/jekich/yii2-nexmo-library"
    },
    "require": {
        "php": ">=5.5",
        "yiisoft/yii2": "^2.0",
        "guzzlehttp/guzzle": "^6.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "dosamigos\\nexmo\\": "src"
        }
    },
    "config": {
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            }
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
